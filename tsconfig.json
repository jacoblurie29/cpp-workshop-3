    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./build",
    "rootDir": "./",
    "target": "es6",
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
  }
}
